package accounts;

import java.util.Random;
import java.util.Scanner;

import Menues.Submit_IT_Docket;

public class Technicians {
	
	private final static String[] POSSIBLE_VALUES = { "Harry Styles", "Niall Horan", "Liam Payne"};
    private final static Random random = new Random();
	
	private final static String[] POSSIBLE_VALUES2 = { "Louis Tomlinson", "Zayn Malik"};
    private final static Random random2 = new Random();
   
    private final static String[] technicianName = {"Harry Styles", "Niall Horan", "Liam Payne",  "Louis Tomlinson", "Zayn Malik"};
	

	public static String technicianLvl;
	public static String  technicianPass;


	public static void main(String[] args) {
		
		technicianPass = "Technician_admin";

	}
	public Technicians(String[] technicianName, String technicianPass) {
		// TODO Auto-generated constructor stub
		this.technicianPass = technicianPass;
	}

	public static String[] getName() {
		return technicianName;
	}

	public static String getPass() {
		return technicianPass;
	}

    public static String getRandomValue() {
        return POSSIBLE_VALUES[random.nextInt(POSSIBLE_VALUES.length)];
    }
    public static String getRandomValue2() {
        return POSSIBLE_VALUES2[random2.nextInt(POSSIBLE_VALUES2.length)];
    }
}
